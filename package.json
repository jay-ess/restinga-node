  },
  "homepage": "https://github.com/jay-ess/restinga-node#readme",
  "devDependencies": {
    "ava": "^0.15.2",
    "babel-cli": "^6.10.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.9.0",
    "coveralls": "^2.11.11",
    "nyc": "^7.1.0-candidate3",
    "xo": "^0.16.0"
  },
  "dependencies": {
    "got": "^6.3.0"
  }
}
