{
  "name": "restinga-node",
  "version": "1.0.0",
  "description": "NodeJS version of artesaos/restinga",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "test": "xo src/**/*.js && ava test/**/*.test.js --require babel-register",
    "test-with-coverage": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "build": "babel src --out-dir lib",
    "prepublish": "npm run build",
    "postpublish": "rm -rf lib/",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jay-ess/restinga-node.git"
  },
  "keywords": [
    "REST",
    "framework"
  ],
  "author": "jay-ess (https://github.com/jay-ess/restinga-node)",
  "contributors": [
